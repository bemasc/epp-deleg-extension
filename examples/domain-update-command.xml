<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <update>
      <domain:update
        xmlns:domain="urn:ietf:params:xml:ns:domain-1.0">
        <domain:name>example.com</domain:name>
      </domain:update>
    </update>
    <extension>
      <deleg:update xmlns:deleg="urn:ietf:params:xml:ns:epp:deleg-0.01">
        <deleg:add>
          <deleg:deleg priority="1" target="ns3.example.org">
            <deleg:params
              alpn="dot"
              tlsa="2 0 1 XYZ..."
              ipv4hint="192.0.2.3"
              ipv6hint="2001:DB8::3"/>
          </deleg:deleg>
        </deleg:add>
        <deleg:rem>
          <deleg:deleg priority="0" target="ns1.example.com">
            <deleg:params
              alpn="dot"
              tlsa="2 0 1 ABC..."
              ipv4hint="192.0.2.1"
              ipv6hint="2001:DB8::1"/>
          </deleg:deleg>
        </deleg:rem>
      </deleg:update>
    </extension>
    <clTRID>ABC-12345</clTRID>
  </command>
</epp>
